<body>
    <section class="my-5">
            <div class="py-5">
            <h1 class="text-center">ADMIN LOGIN</h1>
            </div>
    <div class = "w-50 m-auto">
    <form action ="/auth/data" method="post">
    
            <div class="form-group">
            <label for="pwd">Username</label>
            <input type="text-center" placeholder="Enter Username" id="name" name ="name" autocomplete="off" value="" class="form-control" >
            </div>

            <div class="form-group">
            <label for="pwd">Password</label>
            <input type="Tempature" id="email" name="email" placeholder="Enter Password" autocomplete="off" class="form-control" value = "" >
            </div>
    <button type="save" class="btn btn-success">LOGIN</button>
    </form>
    </div>
    </section>
    </div>

